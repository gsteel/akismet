{
    "name": "gsteel/akismet",
    "description": "Simple Akismet API Bindings",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "akismet",
        "spam",
        "psr-17",
        "psr-18",
        "api-client"
    ],
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "require": {
        "php": "^7.3 || ~8.0 || ~8.1",
        "ext-json": "*",
        "myclabs/php-enum": "^1.8.3",
        "php-http/discovery": "^1.14.1",
        "psr/container": "^1.0 || ^2.0",
        "psr/http-client": "^1.0.1",
        "psr/http-client-implementation": "*",
        "psr/http-factory": "^1.0.1",
        "psr/http-factory-implementation": "*",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0",
        "ergebnis/composer-normalize": "^2.20",
        "laminas/laminas-diactoros": "^2.13",
        "php-http/client-common": "^2.5",
        "php-http/curl-client": "^2.2",
        "php-http/mock-client": "^1.5",
        "phpunit/phpunit": "^9.5.21",
        "symfony/options-resolver": "^5.4.3"
    },
    "autoload": {
        "psr-4": {
            "GSteel\\Akismet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GSteel\\Akismet\\Test\\": "test/Unit",
            "GSteel\\Akismet\\IntegrationTest\\": "test/Integration"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
